-------
CRIAÇÃO DO BANCO DE DADOS SENAI ERP
-------

create database senai_erp;

DROP DATABASE SENAI_ERP;

-------
USANDO O BANCO DE DADOS SENAI ERP
-------

use senai_erp;

-------
## CRIAÇÃO DE TABELAS SEM CHAVES ESTRAGEIRAS
-------

create table GRUP0_PRODUTO(
ID int auto_increment primary key,
NOME VARCHAR(20),
DESCRIÇÃO TEXT
);

CREATE TABLE UNIDADE_PRODUTO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(10),
    DESCRIÇÃO TEXT
);


CREATE TABLE CARGO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT,
    SALARIO DOUBLE(11,2)
);

CREATE TABLE TIPO_COLABORADOR(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);

CREATE TABLE NIVEL_FORMACAO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);

CREATE TABLE FUNCAO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    DESCRICAO_MENU VARCHAR(30),
    IMAGEM_MENU VARCHAR(30),
    METODO VARCHAR(30)
);

CREATE TABLE PAPEL(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);


CREATE TABLE TIPO_RELACIONAMENTO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);

CREATE TABLE TIPO_EMAIL(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);

CREATE TABLE TIPO_TELEFONE(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);

CREATE TABLE TIPO_ENDERECO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);

CREATE TABLE CFOP(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    CFOP INT,
    DESCRICAO TEXT,
    APLICACAO TEXT
);

CREATE TABLE OPERADORA_CARTAO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    BANDEIRA VARCHAR(30),
    NOME VARCHAR(50)
);

CREATE TABLE PAIS(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    CODIGO INT,
    NOME VARCHAR(100),
    SIGLA2 CHAR(2),
    SIGLA3 CHAR(3)
);

CREATE TABLE SITUACAO_FOR_CLI(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);

CREATE TABLE BANCO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    CODIGO INT,
    NOME VARCHAR(100),
    URL VARCHAR(255)
);

CREATE TABLE TIPO_FORNECEDOR(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    NOME VARCHAR(20),
    DESCRICAO TEXT
);

-------
## CRIAÇÃO DE TABELAS + CHAVES ESTRAGEIRAS (FOREIGN KEY)
-------

CREATE TABLE PAPEL_FUNCAO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    FUNCAO_ID INT NOT NULL,
    PAPEL_ID INT NOT NULL,
    PODE_CONSULTAR CHAR(1),
    PODE_INSERIR CHAR(1),
    PODE_ALTERAR CHAR(1),
    PODE_EXCLUIR CHAR(1),
    FOREIGN KEY (FUNCAO_ID) REFERENCES FUNCAO(ID),
    FOREIGN KEY (PAPEL_ID) REFERENCES PAPEL(ID)
);

-- SELECT * FROM PAPEL_FUNCAO


CREATE TABLE SUB_GRUPO_PRODUTO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    ID_GRUPO INT NOT NULL,
    NOME VARCHAR(20),
    DESCRICAO TEXT,
    FOREIGN KEY (ID_GRUPO) REFERENCES grupo_produto(ID)
);

-- SELECT * FROM SUB_GRUPO_PRODUTO

CREATE TABLE PRODUTO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    ID_SUB_GRUPO INT NOT NULL,
    ID_UNIDADE INT NOT NULL,
    GTIN CHAR(14),
    NOME_PRODUTO VARCHAR(100),
    DESCRICAO TEXT,
    DESCRICAO_PDV VARCHAR(30),
    VALOR_COMPRA DOUBLE(11,2),
    VALOR_VENDA DOUBLE(11,2),
    QTD_ESTOQUE DOUBLE(11,2),
    ESTOQUE_MIN DOUBLE(11,2),
    ESTOQUE_MAX DOUBLE(11,2),
    EXCLUIDO CHAR(1),
    DATA_CADASTRO DATE,
    FOREIGN KEY (ID_UNIDADE) REFERENCES UNIDADE_PRODUTO(ID),
    FOREIGN KEY (ID_SUB_GRUPO) REFERENCES UNIDADE_PRODUTO(ID)
);

-- SELECT * FROM PRODUTO

CREATE TABLE EMPRESA(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    EMPRESA_ID INT NOT NULL,
    RAZAO_SOCIAL VARCHAR(150),
    NOME_FANTASIA VARCHAR(150),
    CNPJ VARCHAR(14),
    INSCRICAO_ESTADUAL VARCHAR(30),
    INCRICAO_MUNICIPAL VARCHAR(30),
    MATRIZ_FILIAL CHAR(1),
    DATA_CADASTRO DATE,
    FOREIGN KEY (EMPRESA_ID) REFERENCES EMPRESA(ID)
);

-- SELECT * FROM EMPRESA

CREATE TABLE CONVENIO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    ID_EMPRESA INT NOT NULL,
    NOME VARCHAR(100),
    DESCRICAO TEXT,
    DESCONTO DOUBLE(11,2),
    DATA_VENCIMENTO DATE,
    ENDERECO VARCHAR(250),
    CONTATO VARCHAR(30),
    TELEFONE VARCHAR(10),
    EXCLUIDO CHAR(1),
    DATA_CADASTRO DATE,
    FOREIGN KEY (ID_EMPRESA) REFERENCES EMPRESA(ID)
);

-- SELECT * FROM CONVENIO

CREATE TABLE SETOR(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    EMPRESA_ID INT NOT NULL,
    NOME VARCHAR(20),
    DESCRICAO TEXT,
    FOREIGN KEY (EMPRESA_ID) REFERENCES EMPRESA(ID)
);

-- SELECT * FROM SETOR

CREATE TABLE EMPRESA_PRODUTO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    EMPRESA_ID INT NOT NULL,
    PRODUTO_ID INT NOT NULL,
    FOREIGN KEY (EMPRESA_ID) REFERENCES EMPRESA(ID),
    FOREIGN KEY (PRODUTO_ID) REFERENCES PRODUTO(ID)
);

-- SELECT * FROM EMPRESA_PRODUTO

CREATE TABLE FORNECEDOR(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    SITUACAO_FOR_CLI_ID INT NOT NULL,
    TIPO_FORNECEDOR_ID INT NOT NULL,
    ID_EMPRESA INT NOT NULL,
    NOME VARCHAR(150),
    CPF_CNPJ VARCHAR(14),
    RG VARCHAR(20),
    ORGAO_RG VARCHAR(10),
    INSCRICAO_ESTADUAL VARCHAR(30),
    INCRICAO_MUNICIPAL VARCHAR(30),
    DESDE DATE,
    TIPO_PESSOA CHAR(1),
    EXCLUIDO CHAR(1),
    DATA_CADASTRO DATE,
    FOREIGN KEY (SITUACAO_FOR_CLI_ID) REFERENCES SITUACAO_FOR_CLI(ID),
    FOREIGN KEY (TIPO_FORNECEDOR_ID) REFERENCES TIPO_FORNECEDOR(ID),
    FOREIGN KEY (ID_EMPRESA) REFERENCES EMPRESA(ID)
);

-- SELECT * FROM FORNECEDOR

CREATE TABLE CLIENTE(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    SITUACAO_FOR_CLI_ID INT NOT NULL,
    ID_EMPRESA INT NOT NULL,
    NOME VARCHAR(150),
    CPF_CNPJ VARCHAR(14),
    RG VARCHAR(20),
    ORGAO_RG VARCHAR(10),
    INSCRICAO_ESTADUAL VARCHAR(30),
    INSCRICAO_MUNICIPAL VARCHAR(30),
    DESDE DATE,
    TIPO_PESSOA CHAR(1),
    EXCLUIDO CHAR(1),
    DATA_CADASTRO DATE,
    FOREIGN KEY (SITUACAO_FOR_CLI_ID) REFERENCES SITUACAO_FOR_CLI(ID),
    FOREIGN KEY (ID_EMPRESA) REFERENCES EMPRESA(ID)
);

-- SELECT * FROM CLIENTE

CREATE TABLE FORNECEDOR_PRODUTO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    FORNECEDOR_ID INT NOT NULL,
    PRODUTO_ID INT NOT NULL,
    FOREIGN KEY (FORNECEDOR_ID) REFERENCES FORNECEDOR(ID),
    FOREIGN KEY (PRODUTO_ID) REFERENCES PRODUTO(ID)
);

SELECT * FROM BANCO

CREATE TABLE INDICE_ECONOMICO
(
    ID        INT AUTO_INCREMENT PRIMARY KEY,
    PAIS_ID   INT NOT NULL,
    SIGLA     VARCHAR(10),
    NOME      VARCHAR(50),
    DESCRICAO TEXT,
    FOREIGN KEY (PAIS_ID) REFERENCES PAIS (ID)
);


CREATE TABLE ESTADO
(
    ID        INT AUTO_INCREMENT PRIMARY KEY,
    PAIS_ID   INT NOT NULL,
    SIGLA     VARCHAR(10),
    NOME      VARCHAR(50),
    CODIGO_IBGE INT,
    FOREIGN KEY (PAIS_ID) REFERENCES PAIS(ID)
);


CREATE TABLE CIDADE
(
    ID        INT AUTO_INCREMENT PRIMARY KEY,
    ESTADO_ID   INT NOT NULL,
    NOME      VARCHAR(50),
    CODIGO_IBGE INT,
    FOREIGN KEY (ESTADO_ID) REFERENCES ESTADO(ID)
);


CREATE TABLE CEP
(
    ID        INT AUTO_INCREMENT PRIMARY KEY,
    CIDADE_ID   INT NOT NULL,
    CEP      VARCHAR(8),
    LOGRADOURO VARCHAR(100),
    BAIRRO VARCHAR(50),
    FOREIGN KEY (CIDADE_ID) REFERENCES CIDADE(ID)
);


CREATE TABLE AGENCIA_BANCO
(
    ID        INT AUTO_INCREMENT PRIMARY KEY,
    CEP_ID INT NOT NULL,
    BANCO_ID INT NOT NULL,
    CODIGO INT,
    NOME VARCHAR(100),
    ENDERECO VARCHAR(100),
    TELEFONE VARCHAR(10),
    GERENTE VARCHAR(30),
    CONTATO VARCHAR(30),
    OBS TEXT,
    FOREIGN KEY (BANCO_ID) REFERENCES BANCO(ID)
);


CREATE TABLE COLABORADOR
(
    ID        INT AUTO_INCREMENT PRIMARY KEY,
    NIVEL_FORMACAO_ID INT NOT NULL,
    TIPO_COLABORADOR_ID INT NOT NULL,
    CARGO_ID INT NOT NULL,
    ID_SETOR INT NOT NULL,
    NOME VARCHAR(150),
    CPF VARCHAR(11),
    RG VARCHAR(20),
    ORGAO_RG VARCHAR(10),
    DATA_NASCIMENTO DATE,
    TIPO_SANGINEO VARCHAR(5),
    FOTO_34 VARCHAR(255),
    EXCLUIDO CHAR(1),
    DATA_CADASTRO DATE,
    FOREIGN KEY (ID_SETOR) REFERENCES SETOR(ID),
    FOREIGN KEY (CARGO_ID) REFERENCES CARGO(ID),
    FOREIGN KEY (TIPO_COLABORADOR_ID) REFERENCES TIPO_COLABORADOR(ID),
    FOREIGN KEY (NIVEL_FORMACAO_ID) REFERENCES NIVEL_FORMACAO(ID)
);


CREATE TABLE USUARIO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    PAPEL_ID INT NOT NULL,
    COLABORADOR_ID INT NOT NULL,
    LOGIN VARCHAR(20),
    SENHA VARCHAR(20),
    DATA_CADASTRO DATE,
    FOREIGN KEY (PAPEL_ID) REFERENCES PAPEL(ID),
    FOREIGN KEY (COLABORADOR_ID) REFERENCES COLABORADOR(ID)
);


CREATE TABLE COLABORADOR_RELACIONAMENTO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    TIPO_RELACIONAMENTO_ID INT NOT NULL,
    COLABORADOR_ID INT NOT NULL,
    NOME VARCHAR(100),
    FOREIGN KEY (TIPO_RELACIONAMENTO_ID) REFERENCES TIPO_COLABORADOR(ID),
    FOREIGN KEY (COLABORADOR_ID) REFERENCES COLABORADOR(ID)
);


CREATE TABLE CONTATO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    EMPRESA_ID INT NOT NULL,
    COLABORADOR_ID INT NOT NULL,
    CLIENTE_ID INT NOT NULL,
    FORNECEDOR_ID INT NOT NULL,
    NOME VARCHAR(100),
    DONO CHAR(1),
    FOREIGN KEY (EMPRESA_ID) REFERENCES EMPRESA(ID),
    FOREIGN KEY (COLABORADOR_ID) REFERENCES COLABORADOR(ID),
    FOREIGN KEY (CLIENTE_ID) REFERENCES CLIENTE(ID),
    FOREIGN KEY (FORNECEDOR_ID) REFERENCES FORNECEDOR(ID)
);


CREATE TABLE CONTATO_EMAIL(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    TIPO_EMAIL_ID INT NOT NULL,
    CONTATO_ID INT NOT NULL,
    EMAIL VARCHAR(100),
    FOREIGN KEY (TIPO_EMAIL_ID) REFERENCES TIPO_EMAIL(ID),
    FOREIGN KEY (CONTATO_ID) REFERENCES CONTATO(ID)
);


CREATE TABLE CONTATO_TELEFONE(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    TIPO_TELEFONE_ID INT NOT NULL,
    CONTATO_ID INT NOT NULL,
    TELEFONE VARCHAR(10),
    FOREIGN KEY (TIPO_TELEFONE_ID) REFERENCES TIPO_TELEFONE(ID),
    FOREIGN KEY (CONTATO_ID) REFERENCES CONTATO(ID)
);


CREATE TABLE ENDERECO(
    ID INT AUTO_INCREMENT PRIMARY KEY,
    EMPRESA_ID INT NOT NULL,
    COLABORADOR_ID INT NOT NULL,
    FORNECEDOR_ID INT NOT NULL,
    CLIENTE_ID INT NOT NULL,
    TIPO_ENDERECO_ID INT NOT NULL,
    CEP_ID INT NOT NULL,
    LOGRADOURO VARCHAR(100),
    NUMERO INT,
    COMPLEMENTO VARCHAR(100),
    BAIRRO VARCHAR(50),
    DONO CHAR(1),
    FOREIGN KEY (CEP_ID) REFERENCES CEP(ID),
    FOREIGN KEY (TIPO_ENDERECO_ID) REFERENCES TIPO_ENDERECO(ID),
    FOREIGN KEY (CLIENTE_ID) REFERENCES CLIENTE(ID),
    FOREIGN KEY (FORNECEDOR_ID) REFERENCES FORNECEDOR(ID),
    FOREIGN KEY (COLABORADOR_ID) REFERENCES COLABORADOR(ID),
    FOREIGN KEY (EMPRESA_ID) REFERENCES EMPRESA(ID)
);


